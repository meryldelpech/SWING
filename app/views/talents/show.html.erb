

<p><%= @talent.price %></p>

<p>Booking dates & hours</p>
<%= simple_form_for [@talent, @reservation] do |f| %>
  <%= f.input :date %>
  <%= f.input :sono, as: :check_boxes, collection: ["I provide the sound material"] %>
  <%= f.submit "Book", class: "btn btn-primary" %>
  <%= f.button :button, "Reset", type: "reset", class: "btn-outline-secondary" %>
<% end %>




